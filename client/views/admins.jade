template(name="admins")
  .container-fluid
    .container.content-block
      +each users
        p
          | {{this.services.google.email}}
          if isAdmin this._id
            button.btn.btn-primary.remove-admin(user-id = this._id) Remove admin
          else
            button.btn.btn-primary.make-admin(user-id = this._id) Make admin
