template(name="download")
  .container.download-file.col-md-6.col-md-offset-3
    a(href="{{this}}" download="eidr.csv" target="_blank")
      span.link-text CSV file
      +downloadIcon
  .container.download-file.col-md-6.col-md-offset-3
    a(href="{{this}}" download="eidr.json" target="_blank")
      span.link-text JSON file
      +downloadIcon

template(name="downloadIcon")
  .download-icon
    span.glyphicon.glyphicon-download